{
  "name": "deep-enum-core",
  "version": "0.0.2",
  "description": "Make deeply nested enums out of any constant object with a full type-safe interface.",
  "author": "Sean Blonien",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanblonien/deep-enum-core.git"
  },
  "bugs": {
    "url": "https://github.com/seanblonien/deep-enum-core/issues"
  },
  "homepage": "https://github.com/seanblonien/deep-enum-core#readme",
  "keywords": [
    "deep",
    "enum",
    "enumeration",
    "path",
    "constant",
    "object",
    "typescript",
    "type-safe",
    "nested",
    "json",
    "deeply",
    "dot",
    "notation",
    "immutable"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "run-s clean test:ci build",
    "prepare": "ts-patch install -s",
    "clean": "rimraf dist out",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:ci": "run-p typecheck lint test",
    "test:w": "jest --watch",
    "lint": "eslint --cache --fix --ext .cjs,.ts .",
    "dev": "nodemon --watch src --exec \"run-s build\"",
    "benchmark": "tsc -p tsconfig.benchmark.json && node --experimental-specifier-resolution=node out/scripts/benchmark",
    "size": "tsc -p tsconfig.size.json && node out/package-size"
  },
  "devDependencies": {
    "@types/get-value": "^3.0.2",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.178",
    "@types/object-path": "^0.11.1",
    "@types/set-value": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chalk": "^5.0.0",
    "chalk-template": "^0.3.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-export-all": "^1.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "get-value": "^3.0.1",
    "globby": "^13.1.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "object-path-immutable": "^4.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "set-value": "^4.1.0",
    "ts-jest": "^27.1.2",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest/presets/js-with-ts-esm",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
