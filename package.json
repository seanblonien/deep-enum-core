{
  "name": "deep-enum-core",
  "version": "0.0.2",
  "description": "Make deeply nested enums out of any constant object with a full type-safe interface.",
  "author": "Sean Blonien",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanblonien/deep-enum-core.git"
  },
  "bugs": {
    "url": "https://github.com/seanblonien/deep-enum-core/issues"
  },
  "homepage": "https://github.com/seanblonien/deep-enum-core#readme",
  "keywords": [
    "deep",
    "enum",
    "enumeration",
    "path",
    "constant",
    "object",
    "typescript",
    "type-safe",
    "nested",
    "json",
    "deeply",
    "dot",
    "notation",
    "immutable"
  ],
  "scripts": {
    "prebuild": "npm run typecheck && npm run lint && npm run test",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:w": "jest --watch",
    "lint": "eslint --cache --fix --ext .cjs,.ts .",
    "dev": "nodemon --watch src --exec \"tsc -p tsconfig.build.json\"",
    "size": "tsc -p tsconfig.size.json && node out/package-size.js"
  },
  "dependencies": {
    "object-path-immutable": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-export-all": "^1.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "globby": "^13.1.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest/presets/js-with-ts-esm",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
